<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Hbs HTML</title>
</head>
<body>

<h1>Hello from HBS</h1>

<h3>Hello {{userName}}</h3>

<!--loop example-->
{{#each users}}
<!--            this = users[i]-->
    <div>{{this.name}}</div>
    <div>{{this.age}}</div>
    <div>{{this.password}}</div>
    <hr>
{{/each}}

</body>
</html>